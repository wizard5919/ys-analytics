{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "250mb",
        "includeFiles": "app.py pages/** utils/** assets/**"
      }
    }
  ],
  "functions": {
    "app.py": {
      "memory": 3008,
      "maxDuration": 30,
      "runtime": "python@3.9"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
